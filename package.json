{
	"name": "node-red-sfe",
	"description": "A Single File Executable framework for Node-RED",
	"version": "0.8.0",
	"keywords": [
		"Node-RED",
		"Executable",
		"SFE",
		"Single",
		"Native",
		"Application",
		"Javascript",
		"NodeJS"
	],
	"dependencies": {
		"@yao-pkg/pkg": "^6.2.0",
		"esbuild": "^0.24.2",
		"node-red": "4.0.8",
		"open": "^8.4.2",
		"rimraf": "^5.0.0",
		"archiver": "^7.0.1",
		"adm-zip": "^0.5.16",
		"args-parser": "1.3.0"
	},
	"devDependencies": {
		"eslint": "^9.18.0",
		"prettier": "^3.4.2"
	},
	"scripts": {
		"develop": "node node-red.js --develop",
		"build": "npm run clean && node build.js && npm run package",
		"clean": "rimraf ./build/",
		"package": "cd ./build && pkg ./package.json --compress gzip --targets host --output ./dist/node-red-sfe"
	}
}
